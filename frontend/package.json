    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "@mdi/font": "5.9.55",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.2",
    "jquery": "^3.6.1",
    "mdb-vue-ui-kit": "^3.0.0",
    "pinia": "^2.0.23",
    "popper.js": "^1.16.1",
    "roboto-fontface": "*",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue3-carousel": "^0.2.6",
    "vue3-google-login": "^2.0.12",
    "webfontloader": "^1.0.0"
